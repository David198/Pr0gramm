<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="ThemeControlForegroundBaseHighBrush" Color="{ThemeResource SystemBaseHighColor}" />
    <Color x:Key="ProgrammBaseColor" >#FFD5372A</Color>
    <Color x:Key="LinkColor" >#75c0c7</Color>
    <SolidColorBrush x:Key="ProgrammBaseColorBrush" Color="{StaticResource ProgrammBaseColor}" />
    <SolidColorBrush x:Key="LinkColorBrush" Color="{StaticResource LinkColor}" />
</ResourceDictionary>
